# tleap file generated by amber.build
source ff0_leaprc.protein.ff14SB
source ff1_leaprc.lipid21
source ff2_leaprc.gaff2
source ff3_leaprc.RNA.Shaw
source ff4_leaprc.DNA.bsc1
source ff5_leaprc.water.tip3p

# Loading topologies
loadamberprep topo0_ADP.prepi
loadamberprep topo1_AMP.prepi
loadamberprep topo2_ATP.prepi
loadamberprep topo3_FMN.prepi
loadamberprep topo4_GDP.prepi
loadamberprep topo5_GTP.prepi
loadamberprep topo6_HEM.prepi
loadamberprep topo7_NAD.prepi
loadamberprep topo8_NAI.prepi
loadamberprep topo9_NAP.prepi

# Loading parameter files
loadamberparams param0_ADP.frcmod
loadamberparams param1_AMP.frcmod
loadamberparams param2_ATP.frcmod
loadamberparams param3_FMN.frcmod
loadamberparams param4_GDP.frcmod
loadamberparams param5_GTP.frcmod
loadamberparams param6_HEM.frcmod
loadamberparams param7_NAD.frcmod
loadamberparams param8_NAI.frcmod
loadamberparams param9_NAP.frcmod

# Loading the system
mol = loadpdb input.pdb

setBox mol "vdw"

# Writing out the results
saveamberparm mol structure.prmtop structure.crd
quit