set {
	reference rks
	basis cc-pvdz
}


set_num_threads( 8 )
memory 2 gb

molecule MOL {
0 1

O	 -0.724050	 0.052603	 0.031450
O	 0.724050	 -0.052597	 -0.031450
H	 -0.917150	 -0.768697	 -0.459650
H	 0.917150	 0.768603	 0.459650

	symmetry c1
}
set optking { dynamic_level = 1 }
ee,wfn = energy('b3lyp-d3', return_wfn=True)
oeprop( wfn, 'DIPOLE', 'QUADRUPOLE', 'MULLIKEN_CHARGES')
oeprop( wfn, 'GRID_ESP')
f=open( 'psi4out.xyz', 'w' )
f.write( "4  " )
f.write( str(ee) + "\n" )
f.write( MOL.save_string_xyz() )
f.close()
